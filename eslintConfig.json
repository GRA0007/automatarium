{
  "env": {
    "es2021": true
  },
  "extends": [
    "standard",
    "ts-standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "import/no-absolute-path": "off"
  }
}